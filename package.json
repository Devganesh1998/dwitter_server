{
  "name": "dwitter_server",
  "version": "1.0.0",
  "description": "Social platform with similar features of twitter.",
  "main": "src/index.js",
  "keywords": [
    "express",
    "postgres",
    "redis",
    "elasticsearch",
    "SQL",
    "node",
    "typescript"
  ],
  "repository": "https://github.com/Devganesh1998/dwitter_server.git",
  "author": {
    "name": "Dev Ganesh Pandian",
    "email": "devganesh91@gmail.com",
    "url": "https://github.com/devganesh1998"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon",
    "build": "rm -rf dist && tsc",
    "prebuild": "yarn lint",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint --fix . --ext .ts",
    "seq-mi": "yarn build && IS_JS=true sequelize db:migrate",
    "seq-mi-un": "yarn build && IS_JS=true sequelize db:migrate:undo",
    "seq-mi-una": "yarn build && IS_JS=true sequelize db:migrate:undo:all",
    "seq-mi-ls": "yarn build && IS_JS=true sequelize db:migrate:status",
    "seq-se": "yarn build && IS_JS=true sequelize db:seed",
    "seq-sea": "yarn build && IS_JS=true sequelize db:seed:all",
    "seq-se-un": "yarn build && IS_JS=true sequelize db:seed:undo",
    "seq-se-una": "yarn build && IS_JS=true sequelize db:seed:undo:all",
    "seq-mi-ge": "sequelize migration:generate --name",
    "seq-se-ge": "sequelize seed:generate --name",
    "seq-mo-ge": "sequelize model:generate --name"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.3",
    "@types/redis": "^2.8.29",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "cors": "^2.8.5",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
